{
  "name": "processor-mock",
  "private": "true",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm --workspace apps/processor-mock run dev",
    "dev:processor": "npm --workspace apps/processor-mock run dev",
    "api:generate": "npm --workspace apps/api exec prisma generate",
    "api:migrate": "npm --workspace apps/api exec prisma migrate dev --name manual",
    "api:typecheck": "tsc -p apps/api/tsconfig.json --noEmit"
  },
  "dependencies": {
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@prisma/client": "^6.19.0",
    "abstract-logging": "^2.0.1",
    "bcrypt": "^6.0.0",
    "fastify": "^4.29.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^10.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "prisma": "^6.19.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "type": "module"
}
